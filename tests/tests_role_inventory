#!/usr/bin/env bash_unit

[ -z "${RU_ENV_IMAGE}" ] && export RU_ENV_IMAGE=debian9
export RU_ENV_NAME=$(date +%s)
export RU_FIXED_NAMES=1
export RU_COUNT=2

. lib/role_unit

test_i_can_get_machines_with_fixed_names() {
  assert_equals "${ru_env_name}_00" "$(ru_server_name 0)"
  assert_equals "${ru_env_name}_01" "$(ru_server_name 1)"
}

setup_suite() {
  ru_init
}

# vim: syntax=sh
